<script type="text/javascript">	
	//为所有的li添加单击事件 阻止冒泡
	$("li").click(function(e){
		e.stopPropagation();
	})
	//为有子元素的li添加单击事件
	$(".navigation:has(ul)").click(function(){
		//判断当前操作的li的子元素ul是否是隐藏  如果隐藏 就显示   否则就隐藏
		if( $(this).children("ul").css("display") == "none" ){
			$(this).css("background","#454545");
			$(this).children("em").attr("class","iconfont icon-jianhao")
		}else{
			$(this).css("background","#575757");
			$(this).children("em").attr("class","iconfont icon-jiahao")
		}
		$(this).children("ul").slideToggle(1000);
	})
</script>